{% extends "_layout.twig" %} 
{% set bodyAttributes ={ class: "other" } %} 
{% set docTitle = entry.title %} 
{% set metaDesc = entry.metaDescription %} 
{% set page = craft.entries().section('home').all() %} 

{% set buttonClass = "green-lime" %}
{% set buttonClassContrast = "green-lime" %}

{% block content %} 
  {% for entry in page %} 
    {% include '_partials/hero/environment.twig' %}
    {% include '_partials/sections/enviro-mask.twig' %}
    {% include '_partials/sections/enviro-banner.twig' %}
    {% embed '_partials/sections/enviro-contrast.twig' %}
        {% block class %} bg-lime {% endblock %}
        {% block colCount %} third {% endblock %}
    {% endembed %}
    {% embed '_partials/sections/enviro-contrast-btm.twig' %}
        {% block class %} bg-lime addons{% endblock %}
        {% block colCount %} half {% endblock %}
    {% endembed %}

  {% endfor %} 
{% endblock %}