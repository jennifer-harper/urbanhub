{% extends "_layout.twig" %} 
{% set bodyAttributes ={ class: "sustain-wellbeing greenTheme svgStyle" } %} 
{% set docTitle = entry.title %} 
{% set metaDesc = entry.metaDescription %} 
{% set page = craft.entries().section('sustainabilityWellbeing').all() %} 


{% set buttonClass = "green" %}
{% set buttonClassContrast = "green" %}
{% set main = "bg-green-dark" %}
{% set contrast = "bg-green-light" %}
{% set footer = "bg-green-light" %}



{% block content %} 
  {% for entry in page %} 
        {% include '_partials/hero/centered-hero.twig' %}
        {% embed '_partials/sections/star-banner-section.twig' %}
            {% block class %} bg-light {% endblock %}
        {% endembed %}
        <div class="solutions-contrast wellbeing bg-green-dark dark">
        <div class="something">
                {% for block in entry.wellbeingSlides %}
                    {% if block.type == 'groupFields' %}
                    <div class=" pad-all flex-wrap panel {% block colCount %} {% endblock %}">
                        <div class="half">
                            {% if block.subHeading %}
                                {{block.subHeading}}
                            {% endif %}
                            <div class="intro-text">
                                {% if block.introParagraph %}
                                    {{block.introParagraph}}
                                {% endif %}
                            </div>

                            <div class="no-btm-marg">
                                {% if block.paragraph %}
                                    {{block.paragraph}}
                                {% endif %}
                            </div>
                        </div>
                            <div class="half">
                            {% if block.icon.one() %}
                            {% set image = block.icon.one() %}
                                <img class="max-50" src="{{ image.url }}" alt="{{ image.alt }}">
                            {% else %}
                                <img class="max-50" src="uploads/images/illustration4-keys.svg" alt="image icon">
                            {% endif %}
                        </div>
                    </div>
                    {% endif %}                           
                {% endfor %}
            </div>
        </div>
    {% include '_partials/sections/solutions-mask.twig' %}
    {% include '_partials/footer/contact-section.twig' %}
  {% endfor %} 
{% endblock %}

{% block scripts %}
{{ parent() }}

    <script src="/assets/js/sustainability-wellbeing.js"></script>
{% endblock %}