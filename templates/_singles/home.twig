{% extends "_layout.twig" %} 
{% set bodyAttributes ={ class: "home orangeTheme" } %} 
{% set docTitle = entry.title %} 
{% set metaDesc = entry.metaDescription %} 
{% set page = craft.entries().section('home').all() %} 

{% set buttonClass = "orange" %}
{% set buttonClassContrast = "yellow" %}

{% block content %} 
  {% for entry in page %} 
    {% include '_partials/hero/environment.twig' %}
    {% include '_partials/sections/enviro-mask.twig' %}





    <div class="pad-all bg-yellow-light">
        <h1 class="lg">hello ambitious <span>home 135px</span></h1>
        <h1>hello H1 95px </h1>
        <h2>hello H2 80px</h2>
        <h3>Hello H3 30px</h3>
        <div ><p class="intro-text">Intro text</p></div>
        <div><p class="header-intro">Header intro</p></div>
        <div><p>Sentence</p> <a href="">Link</a></div>
        <div class="dark"><p>Sentence Dark</p> <a href="">Link</a></div>
        {% embed '_partials/button' %}
        {% block class %}{{buttonClass}}{% endblock %}
        {% endembed %} 
    </div>



     <div class="pad-all">
     {% include '_partials/button' %}

      {% embed '_partials/button' %}
        {% block class %}{{buttonClassContrast}}{% endblock %}
      {% endembed %} 

      {% embed '_partials/button' %}
        {% block class %}purple{% endblock %}
      {% endembed %} 

      {% embed '_partials/button' %}
        {% block class %}blue {% endblock %}
      {% endembed %} 

      {% embed '_partials/button' %}
        {% block class %}yellow {% endblock %}
      {% endembed %} 
      {% embed '_partials/button' %}
        {% block class %}lime{% endblock %}
      {% endembed %}
    </div>
  {% endfor %} 
{% endblock %}