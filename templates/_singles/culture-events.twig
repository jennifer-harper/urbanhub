{% extends "_layout.twig" %} 
{% set bodyAttributes ={ class: "culture-events lightBlueTheme svgStyle" } %} 
{% set docTitle = entry.title %} 
{% set metaDesc = entry.metaDescription %} 
{% set page = craft.entries().section('cultureEvents').all() %} 

{% set buttonClass = "blue" %}
{% set buttonClassContrast = "blue" %}

{# //background color #}
{% set main = "bg-blue-light" %}
{# //alternate star color #}
{% set contrast = "bg-blue-dark" %}
{% set footer = "bg-blue-light" %}



{% block content %} 
  {% for entry in page %} 
    {% include '_partials/sections/solutions-mask.twig' %}

    {% embed '_partials/sections/solutions-contrast-sm.twig' %}
        {% block class %} no-top {% endblock %}
        {% block colCount %} third{% endblock %}
    {% endembed %}

    {% embed '_partials/sections/solutions-icon-grid.twig' %}
        {% block headingAlign %} align-right max-title {% endblock %}
        {% block class %} {{main }}{% endblock %}
        {% block colCount %} third{% endblock %}
    {% endembed %}

    {% include '_partials/sections/events.twig' %}
   {% embed '_partials/footer/social-media.twig' %}
           {% block class %} border-top{% endblock %}
    {% endembed %}
    {% include '_partials/footer/contact-section.twig' %}
  {% endfor %} 
{% endblock %}

