{% extends "_layout.twig" %} 
{% set bodyAttributes ={ class: "the-team purpleTheme" } %} 
{% set docTitle = entry.title %} 
{% set metaDesc = entry.metaDescription %} 
{% set page = craft.entries().section('theTeam').all() %} 

{% set buttonClass = "purple" %}
{% set buttonClassContrast = "purple" %}

{# //background color #}
{% set main = "bg-purple-dark" %}
{# //alternate star color #}
{% set contrast = "bg-purple-dark" %}
{% set footer = "bg-purple-light" %}



{% block content %} 
  {% for entry in page %} 
   {% include '_partials/sections/solutions-mask.twig' %}
    {% embed '_partials/sections/center-banner.twig' %}

    {% endembed %}

    {% include '_partials/sections/star-banner-section.twig' %}
    {% embed '_partials/sections/solutions-contrast.twig' %}
        {% block class %} {{main }} dark {% endblock %}
        {% block colCount %} third{% endblock %}
    {% endembed %}
    {% include '_partials/sections/staff.twig' %}
    {% include '_partials/footer/contact-section.twig' %}
  {% endfor %} 
{% endblock %}

