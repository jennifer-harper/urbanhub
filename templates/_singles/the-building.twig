{% extends "_layout.twig" %} 
{% set bodyAttributes ={ class: "the-building lightBlueTheme svgStyle" } %} 
{% set docTitle = entry.title %} 
{% set metaDesc = entry.metaDescription %} 
{% set page = craft.entries().section('theBuilding').all() %} 

{% set buttonClass = "blue" %}
{% set buttonClassContrast = "blue" %}

{# //background color #}
{% set main = "bg-blue-light" %}
{# //alternate star color #}
{% set contrast = "bg-blue-dark" %}
{% set footer = "bg-blue-light" %}



{% block content %} 
  {% for entry in page %} 
    {% embed '_partials/hero/centered-hero.twig' %}
        {% block class %} {{main }}{% endblock %}
    {% endembed %}


    {% embed '_partials/sections/solutions-mask.twig' %}
        {% block pageClass %} bg-light {% endblock %}
    {% endembed %} 


    {% embed '_partials/sections/centered-section.twig' %}
        {% block class %} bg-light {% endblock %}
    {% endembed %}

    {% embed '_partials/sections/star-banner-section.twig' %}
        {% block class %} bg-light {% endblock %}
    {% endembed %} 

    {% embed '_partials/sections/solutions-contrast-sm.twig' %}
        {% block class %} {{main }}{% endblock %}
        {% block colCount %} third{% endblock %}
    {% endembed %}


    {% embed '_partials/footer/social-media.twig' %}
        {% block css %}bg-light {% endblock %}
    {% endembed %} 

    {% embed '_partials/footer/contact-section.twig' %}
        {% block class %}bg-light {% endblock %}
    {% endembed %} 




     {% embed '_partials/footer/map.twig' %}
        {% block class %}pad-all no-top bg-light{% endblock %}
     {% endembed %}
  {% endfor %} 
{% endblock %}

