{% extends "_layout.twig" %} 
{% set bodyAttributes ={ class: "the-building lightBlueTheme" } %} 
{% set docTitle = entry.title %} 
{% set metaDesc = entry.metaDescription %} 
{% set page = craft.entries().section('theBuilding').all() %} 

{% set buttonClass = "blue" %}
{% set buttonClassContrast = "blue" %}

{# //background color #}
{% set main = "bg-blue-light" %}
{# //alternate star color #}
{% set contrast = "bg-blue-dark" %}
{% set footer = "bg-blue-light" %}



{% block content %} 
  {% for entry in page %} 
    {% embed '_partials/hero/centered-hero.twig' %}
        {% block class %} {{main }}{% endblock %}
    {% endembed %}
    {% include '_partials/sections/solutions-mask.twig' %}
    {% include '_partials/sections/centered-section.twig' %}
    {% include '_partials/sections/star-banner-section.twig' %}
    {% embed '_partials/sections/solutions-contrast-sm.twig' %}
        {% block class %} {{main }}{% endblock %}
        {% block colCount %} third{% endblock %}
    {% endembed %}
    {% include '_partials/footer/social-media.twig' %}
    {% include '_partials/footer/contact-section.twig' %}
     {% include '_partials/footer/map.twig' %}
  {% endfor %} 
{% endblock %}

