   
{% extends "_layout.twig" %} 
{% set bodyAttributes ={ class: "urbanEnvironment greenTheme" } %} 
{% set docTitle = entry.title %} 
{% set metaDesc = entry.metaDescription %} 
{% set page = craft.entries().section('urbanEnvironment').all() %} 


{% set buttonClass = "green" %}
{% set buttonClassContrast = "green-light" %}

{% set main = "bg-green-dark" %}
{% set contrast = "bg-green-light" %}
{% set footer = "bg-green-light" %}


{% block content %} 
  {% for entry in page %} 
    {% include '_partials/hero/centered-hero.twig' %}
    {% embed '_partials/sections/star-banner-section.twig' %}
        {% block class %} bg-light {% endblock %}
    {% endembed %}

       {% embed '_partials/sections/btn-icon-section.twig' %}
        {% block class %} {{main}} dark {% endblock %}
    {% endembed %}
    


    {% embed '_partials/sections/solutions-mask.twig' %}
    
{% endembed %}

   {% include '_partials/footer/social-media.twig' %}
    {% include '_partials/footer/contact-section.twig' %}
  {% endfor %} 
{% endblock %}

